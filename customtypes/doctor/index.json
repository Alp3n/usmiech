{
  "id": "doctor",
  "label": "Doctor",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "URL slug",
          "placeholder": "Name URL slug..."
        }
      },
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Name",
          "placeholder": "Doctor's name...",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "clinics": {
        "type": "Group",
        "config": {
          "label": "Clinics",
          "fields": {
            "clinic": {
              "type": "Link",
              "config": {
                "label": "Clinic",
                "select": "document"
              }
            }
          }
        }
      },
      "fieldsOfSpecializations": {
        "type": "Group",
        "config": {
          "label": "Fields of Specializations",
          "fields": {
            "specializationName": {
              "type": "StructuredText",
              "config": {
                "label": "Specialization Name",
                "placeholder": "",
                "allowTargetBlank": true,
                "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
              }
            }
          }
        }
      },
      "courses": {
        "type": "Group",
        "config": {
          "label": "Courses",
          "fields": {
            "course": {
              "type": "StructuredText",
              "config": {
                "label": "Course",
                "placeholder": "Enter course information. One line",
                "allowTargetBlank": true,
                "single": "paragraph"
              }
            }
          }
        }
      },
      "finishedEducation": {
        "type": "Group",
        "config": {
          "label": "Finished Education",
          "fields": {
            "education": {
              "type": "StructuredText",
              "config": {
                "label": "Education",
                "placeholder": "Example of finished degrees",
                "allowTargetBlank": true,
                "single": "paragraph"
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "repetable_items": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO": {
      "metaTitle": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": ""
        }
      },
      "metaDescription": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": ""
        }
      }
    }
  }
}